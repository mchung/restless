<xml>
 <action>create_folder</action>
 <data>
   <folder>
     <parent_id>
        @parent_folder@
     </parent_id>
     <name>
        @folder_name@
     </name>
         <sharing>
            <share>@share_enum@</share>
            <p_comments>@p_comments@</p_comments>
            <a_password>@a_password@</a_password>
            <send_to>@send_to@</send_to>
            <e_message>@e_message@</e_message>
            <type_id></type_id>
         </sharing>
   </folder>
 </data>
</xml>